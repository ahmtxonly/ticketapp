{
    "presets": [
        "module:metro-react-native-babel-preset"
    ],
    "plugins": [
        [
            "module-resolver",
            {
                "root": [
                    "./app"
                ],
                "extensions": [
                    ".ios.ts",
                    ".android.ts",
                    ".ts",
                    ".ios.tsx",
                    ".android.tsx",
                    ".jsx",
                    ".js",
                    ".json"
                ],
                "alias": {
                    "@components": "./app/components",
                    "@actions": "./app/actions",
                    "@constants": "./app/constants",
                    "@assets": "./app/assets",
                    "@utils": "./app/utils"
                }
            }
        ]
    ]
}